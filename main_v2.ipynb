            "source": [
                "# 3. ROBUST LAUNCH CELL\n",
                "import os, time, shutil\n",
                "from google.colab import output\n",
                "\n",
                "def launch():\n",
                "    drive_root = '/content/drive/MyDrive/CodingProjects/BreadboardAI'\n",
                "    local_root = '/content/app'\n",
                "    zip_file = os.path.join(drive_root, 'node_modules.zip')\n",
                "    \n",
                "    if not os.path.exists(drive_root):\n",
                "        print(f\"[ERROR] {drive_root} not found.\")\n",
                "        return\n",
                "\n",
                "    if not os.path.exists(local_root): os.makedirs(local_root)\n",
                "    os.chdir(local_root)\n",
                "\n",
                "    # cleanup logs\n",
                "    for f in ['server_log.txt', 'vite_log.txt']: \n",
                "        if os.path.exists(f): os.remove(f)\n",
                "\n",
                "    # node_modules setup\n",
                "    if os.path.exists('node_modules'):\n",
                "        print(\"[INFO] Using local node_modules.\")\n",
                "    elif os.path.exists(zip_file):\n",
                "        print(\"[TASK] Unzipping node_modules...\")\n",
                "        shutil.copy(zip_file, '/content/node_modules.zip')\n",
                "        os.makedirs(os.path.join(local_root, 'node_modules'), exist_ok=True)\n",
                "        get_ipython().system('unzip -q /content/node_modules.zip -d {local_root}/node_modules')\n",
                "    else:\n",
                "        print(\"[WARN] Clean Install...\")\n",
                "        shutil.copy(os.path.join(drive_root, 'package.json'), local_root)\n",
                "        get_ipython().system('npm install --no-audit --no-fund > npm_install.log 2>&1')\n",
                "\n",
                "    print(\"[TASK] Refreshing source code...\")\n",
                "    for i in ['src', 'public', 'vite.config.ts', 'tsconfig.json', 'package.json']:\n",
                "        s, d = os.path.join(drive_root, i), os.path.join(local_root, i)\n",
                "        if os.path.exists(d): shutil.rmtree(d) if os.path.isdir(d) else os.remove(d)\n",
                "        shutil.copytree(s, d) if os.path.isdir(s) else shutil.copy2(s, d)\n",
                "    \n",
                "    get_ipython().system(f\"chmod +x {local_root}/node_modules/.bin/* || true\")\n",
                "    \n",
                "    print(\"[TASK] Launching servers...\")\n",
                "    get_ipython().system(\"fuser -k 3000/tcp 5173/tcp || true\")\n",
                "    os.environ['NODE_PATH'] = os.path.join(local_root, 'node_modules')\n",
                "    get_ipython().system_raw(f'cd {local_root} && npx tsx src/bridge/server.ts > server_log.txt 2>&1 &')\n",
                "    get_ipython().system_raw(f'cd {local_root} && npx vite --port 5173 --host 0.0.0.0 > vite_log.txt 2>&1 &')\n",
                "    \n",
                "    print(\"[WAIT] Waiting for warmup (10s)...\")\n",
                "    time.sleep(10)\n",
                "    \n",
                "    print(\"[INFO] Port Status (Internal VM):\")\n",
                "    get_ipython().system(\"netstat -tuln | grep -E ':3000|:5173'\")\n",
                "    \n",
                "    try:\n",
                "        from google.colab import output\n",
                "        \n",
                "        # Robust multi-path proxy retrieval\n",
                "        proxy_url = None\n",
                "        locations = [\n",
                "            ('google.colab.kernel', 'proxy_port'),\n",
                "            ('google.colab.output', 'proxy_port')\n",
                "        ]\n",
                "        \n",
                "        for mod_name, func_name in locations:\n",
                "            try:\n",
                "                import importlib\n",
                "                mod = importlib.import_module(mod_name)\n",
                "                func = getattr(mod, func_name)\n",
                "                proxy_url = func(5173)\n",
                "                if proxy_url: break\n",
                "            except:\n",
                "                continue\n",
                "        \n",
                "        print(\"\\n\" + \"=\"*50)\n",
                "        print(\" ACCESS YOUR BREADBOARD AI APP \")\n",
                "        print(\"=\"*50)\n",
                "        if proxy_url:\n",
                "            print(f\" ðŸ”— CLICK HERE: {proxy_url}\")\n",
                "        else:\n",
                "            print(\" 1. Go to: https://colab.research.google.com\")\n",
                "            print(\" 2. Look at the top-right toolbar.\")\n",
                "            print(\" 3. Click the 'Web Preview' button (Square with arrow).\")\n",
                "            print(\" 4. Select 'Preview on port 5173'.\")\n",
                "        print(\"=\"*50 + \"\\n\")\n",
                "        \n",
                "        print(\"Attempting to embed preview below (Usually blank in VS Code)...\")\n",
                "        output.serve_kernel_port_as_iframe(5173, path=\"/\", height=800)\n",
                "        \n",
                "    except Exception as e:\n",
                "        print(f\"[ERROR] UI setup failed: {e}\")\n",
                "\n",
                "if 'IN_COLAB' in globals() and IN_COLAB: launch()"
            ]
        }\n
    ],
